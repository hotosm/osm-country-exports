{
    "iso3": "ISO",
    "include_stats": true,
    "include_stats_html": true,
    "hdx_upload": true,
    "categories": [
        {
            "Buildings": {
                "hdx": {
                    "tags": [
                        "facilities-infrastructure",
                        "geodata"
                    ],
                    "caveats": "OpenStreetMap data is crowd sourced and cannot be considered to be exhaustive"
                },
                "types": [
                    "polygons"
                ],
                "select": [
                    "name",
                    "name:en",
                    "building",
                    "building:levels",
                    "building:materials",
                    "addr:full",
                    "addr:housenumber",
                    "addr:street",
                    "addr:city",
                    "office",
                    "source"
                ],
                "where": "tags['building'] IS NOT NULL",
                "formats": [
                    "geojson",
                    "shp",
                    "gpkg",
                    "kml"
                ]
            }
        },
        {
            "Roads": {
                "hdx": {
                    "tags": [
                        "transportation",
                        "roads",
                        "geodata"
                    ],
                    "caveats": "OpenStreetMap data is crowd sourced and cannot be considered to be exhaustive"
                },
                "types": [
                    "lines",
                    "polygons"
                ],
                "select": [
                    "name",
                    "name:en",
                    "highway",
                    "surface",
                    "smoothness",
                    "width",
                    "lanes",
                    "oneway",
                    "bridge",
                    "layer",
                    "source"
                ],
                "where": "tags['highway'] IS NOT NULL",
                "formats": [
                    "geojson",
                    "shp",
                    "gpkg",
                    "kml"
                ]
            }
        },
        {
            "Waterways": {
                "hdx": {
                    "tags": [
                        "hydrology",
                        "rivers",
                        "geodata"
                    ],
                    "caveats": "OpenStreetMap data is crowd sourced and cannot be considered to be exhaustive"
                },
                "types": [
                    "points",
                    "lines",
                    "polygons"
                ],
                "select": [
                    "name",
                    "name:en",
                    "waterway",
                    "covered",
                    "width",
                    "depth",
                    "layer",
                    "blockage",
                    "tunnel",
                    "natural",
                    "water",
                    "source"
                ],
                "where": "tags['waterway'] IS NOT NULL OR tags['water'] IS NOT NULL OR tags['natural'] IN ('water','wetland','bay')",
                "formats": [
                    "geojson",
                    "shp",
                    "gpkg",
                    "kml"
                ]
            }
        },
        {
            "Points of Interest": {
                "hdx": {
                    "tags": [
                        "facilities-infrastructure",
                        "points of interest-poi",
                        "geodata"
                    ],
                    "caveats": "OpenStreetMap data is crowd sourced and cannot be considered to be exhaustive"
                },
                "types": [
                    "points",
                    "polygons"
                ],
                "select": [
                    "name",
                    "name:en",
                    "amenity",
                    "man_made",
                    "shop",
                    "tourism",
                    "opening_hours",
                    "beds",
                    "rooms",
                    "addr:full",
                    "addr:housenumber",
                    "addr:street",
                    "addr:city",
                    "source"
                ],
                "where": "tags['amenity'] IS NOT NULL OR tags['man_made'] IS NOT NULL OR tags['shop'] IS NOT NULL OR tags['tourism'] IS NOT NULL",
                "formats": [
                    "geojson",
                    "shp",
                    "gpkg",
                    "kml"
                ]
            }
        },
        {
            "Cultural Places": {
                "tags": [
                    "cultural",
                    "heritage",
                    "historic-sites",
                    "geodata",
                    "entertainment"
                ],
                "caveats": "OpenStreetMap data is crowd sourced and cannot be considered to be exhaustive",
                "types": [
                    "points",
                    "polygons"
                ],
                "select": [
                    "name",
                    "tourism",
                    "amenity",
                    "historic",
                    "heritage",
                    "operator",
                    "opening_hours",
                    "website",
                    "description",
                    "capacity",
                    "screen"
                ],
                "where": "tags['tourism'] IN ('museum', 'gallery', 'theatre', 'artwork', 'attraction', 'zoo', 'aquarium', 'cinema') OR tags['amenity'] IN ('arts_centre', 'library', 'community_centre', 'theatre', 'fountain', 'planetarium', 'public_bookcase', 'social_centre', 'stage', 'place_of_mourning', 'place_of_worship', 'cinema') OR tags['historic'] IS NOT NULL OR tags['heritage'] IS NOT NULL",
                "formats": [
                    "geojson",
                    "shp",
                    "gpkg",
                    "kml"
                ]
            }
        },
        {
            "Airports": {
                "hdx": {
                    "tags": [
                        "aviation",
                        "facilities-infrastructure",
                        "geodata"
                    ],
                    "caveats": "OpenStreetMap data is crowd sourced and cannot be considered to be exhaustive"
                },
                "types": [
                    "points",
                    "lines",
                    "polygons"
                ],
                "select": [
                    "name",
                    "name:en",
                    "aeroway",
                    "building",
                    "emergency",
                    "emergency:helipad",
                    "operator:type",
                    "capacity:persons",
                    "addr:full",
                    "addr:city",
                    "source"
                ],
                "where": "tags['aeroway'] IS NOT NULL OR tags['building'] = 'aerodrome' OR tags['emergency:helipad'] IS NOT NULL OR tags['emergency'] = 'landing_site'",
                "formats": [
                    "geojson",
                    "shp",
                    "gpkg",
                    "kml"
                ]
            }
        },
        {
            "Sea Ports": {
