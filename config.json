                    "amenity",
                    "building",
                    "port",
                    "operator:type",
                    "addr:full",
                    "addr:city",
                    "source"
                ],
                "where": "tags['amenity'] = 'ferry_terminal' OR tags['building'] = 'ferry_terminal' OR tags['port'] IS NOT NULL",
